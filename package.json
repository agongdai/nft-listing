    "e2e-cmd": "cypress run",
    "e2e-cmd:chrome": "cypress run -b chrome",
    "e2e-cmd:firefox": "cypress run -b firefox",
    "e2e-cmd:edge": "cypress run -b edge",
    "e2e-cmd:smoke": "cypress run -b chrome --env grepTags=@smoke",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "bundle": "npx cra-bundle-analyzer",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|tsx|js|scss|css|svg)\" --write",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
